<h3>How many selected spots?</h3>
<%= @events.count %>
<h3>Final Price</h3>
<%=  (@events.count * 19.99) %>

<h3>Name of mentor</h3>
<%= @user.first_name + " " + @user.last_name %>

<h3>Hours Selecteds</h3>

<% @events.each do  |event| %>
  <p>Start Time</p>
  <%= event.start %>
  <p>End Time</p>
  <%= event.end %>
<% end %>

<button class="btn btn-danger btn-lg">Confirmar</button>


